<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Years_Reqd_Stage_is_Contract</fullName>
    <active>true</active>
    <errorConditionFormula>AND ( ISPICKVAL(StageName, &quot;Contract&quot;),OR(ISBLANK(TEXT(Contract_Years__c))))</errorConditionFormula>
    <errorDisplayField>Contract_Years__c</errorDisplayField>
    <errorMessage>Contract Years must be a number between 1-5</errorMessage>
</ValidationRule>
