<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Count_Down__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status,&quot;WIP&quot;),
  (60 + Contract_Start_Date__c) -(today()),NULL)</formula>
    <inlineHelpText>This field calculates the number of days remaining for the Implementation to complete while the Status = WIP.  The standard is 60 days.</inlineHelpText>
    <label>Count Down</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
