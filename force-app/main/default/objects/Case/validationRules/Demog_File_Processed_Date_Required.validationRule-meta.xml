<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Demog_File_Processed_Date_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND (ISPICKVAL (Demog_File_Status__c, &quot;Complete&quot;), 
ISNULL(Demog_File_Processed_Date__c))</errorConditionFormula>
    <errorDisplayField>Demog_File_Status__c</errorDisplayField>
    <errorMessage>The Demog File Processed Date field must contain a date when the Status = Complete.</errorMessage>
</ValidationRule>
