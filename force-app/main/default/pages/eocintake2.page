                if (email.length != 0) {
                    if (re.test(email)) {
                        $jx('.emailmsg').hide();
                    } else {
                        $jx('.emailmsg').hide();
                        $jx('.error').show();
                        $jx('[id$="parentEmail"]').addClass("error");
